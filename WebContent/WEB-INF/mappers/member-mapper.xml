<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="member">
	<insert id="insert" parameterType="member.model.MemberDTO">		
		insert into member values(#{id},#{pass},#{email},#{phone1},#{phone2},#{phone3},#{name},#{birth},#{address1},#{address2},#{address3},#{address4},#{gender},sysdate)
	</insert>	
</mapper>